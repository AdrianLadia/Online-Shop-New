{
    "printWidth": 120,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "always",
    "proseWrap": "never",
    "semi": true,
    "useTabs": false,
    "overrides": [
      {
        "files": "*.js",
        "options": {
          "max-len": [ "error", { "code": 120 } ],
          "arrowParens": "always",
          "bracketSpacing": true,
          "jsxBracketSameLine": false,
          "semi": true,
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false,
          "quoteProps": "as-needed",
          "function-paren-newline": "multiline",
          "function-call-argument-newline": "multiline",
          "function-multiline": "avoid",
          "call-multiline": "avoid"
        }
      }
    ]
  }
  